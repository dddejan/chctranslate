(set-logic HORN)
(set-info :status sat)
(declare-fun p (Int) Bool)
(declare-fun q (Int) Bool)
(declare-fun r (Int) Bool)
(assert (forall ((x Int)) (=> (> x 0) (p x))))
(assert (forall ((x Int)) (=> (<= x 0) (q x))))
(assert (forall ((x Int)) (=> (and (p x) (q x)) (r x))))
(assert (forall ((x Int)) (=> (r x) (= x 0))))
